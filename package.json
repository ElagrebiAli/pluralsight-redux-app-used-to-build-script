{
  "name": "ps-redux",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "bootstrap": "4.2.1",
    "immer": "1.9.3",
    "prop-types": "15.6.2",
    "react": "next",
    "react-dom": "next",
    "react-redux": "6.0.0",
    "react-router-dom": "next",
    "react-toastify": "4.5.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/register": "7.0.0",
    "autoprefixer": "9.4.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "babel-preset-react-app": "6.1.0",
    "chalk": "2.4.1",
    "css-loader": "2.0.2",
    "cssnano": "4.1.8",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.10.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.11.1",
    "fetch-mock": "7.2.7",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "json-server": "0.14.0",
    "mini-css-extract-plugin": "0.5.0",
    "npm-run-all": "4.1.5",
    "postcss-loader": "3.0.0",
    "react-test-renderer": "16.6.3",
    "react-testing-library": "5.4.2",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "rimraf": "2.6.2",
    "style-loader": "0.23.1",
    "webpack": "4.28.2",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.13"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
}
